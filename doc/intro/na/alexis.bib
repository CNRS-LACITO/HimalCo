@Article{Bird2008,
  Title                    = {Oral-laryngeal timing in glottalised resonants},
  Author                   = {Bird, Sonya and Caldecott, Marion and Campbell, Fiona and Gick, Bryan and Shaw, Patricia A.},
  Journal                  = {Journal of Phonetics},
  Year                     = {2008},
  Number                   = {3},
  Pages                    = {492--507},
  Volume                   = {36},

  Abstract                 = {Although previous studies of intergestural timing in multi-gesture segments have identified some consistent patterns, fundamental questions remain about the underlying causes of these patterns. Hypotheses based on universal perceptual or biomechanical restrictions have proven difficult to test because of confounding factors, e.g. anatomical coupling and aerodynamic necessity. The cross-linguistically rare class of glottalised resonants (GRs) involves oral and laryngeal gestures, which are neither anatomically nor aerodynamically interdependent, thereby providing a revealing test case for these hypotheses. If intergestural timing is determined by universal perceptual factors, GR timing patterns should be consistent across languages. This comparative study of GRs in three endangered British Columbian languages reveals distinct patterns: the timing of GRs is consistently pre-glottalised in Nuu-chah-nulth, post-glottalised in Nɬeʡkepmxcin, and dependent upon syllable position in St{\textquoteright}{\'{a}}t{\textquoteright}imcets. These findings indicate that a strong hypothesis based on perceptual recovery must be rejected, and suggest instead that intergestural timing must be specified on a language-specific basis.}
}

